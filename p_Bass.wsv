    常量 BASS_CONFIG_SPLIT_BUFFER <公开 类型 = 整数 值 = 0x10610 注释 = "【BASS混合器类】分离器的缓冲区长度." 注释 = "参数: 缓冲区长度(单位为毫秒)">
}

类 BASS_配置选项_指针 <公开 注释 = "\"置配置选项_指针/BASS_SetConfigPtr\"的选项" 折叠 @文档 = "category = \"辅助类\"">
