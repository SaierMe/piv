        加入文本 (展示内容, "13. 外投影角度: " + 取调试文本2 (最大展示数据尺寸, 外投影角度) + "\r\n")
        加入文本 (展示内容, "14. 外投影音量: " + 取调试文本2 (最大展示数据尺寸, 外投影音量) + "\r\n")
        加入文本 (展示内容, "15. vam: " + 取调试文本2 (最大展示数据尺寸, vam) + "\r\n")
        加入文本 (展示内容, "16. 优先级: " + 取调试文本2 (最大展示数据尺寸, 优先级) + "\r\n")
    }
}

类 BASS_通道信息 <公开 基础类 = 扩展对象类 注释 = "\"取通道信息/BASS_ChannelGetInfo\"返回的信息" 折叠 @文档 = "category = \"辅助类\"">
