    # 内部方法 ---

    方法 线程类_线程运行 <接收事件 类型 = 整数 注释 = "  用户需要接收并处理本事件以执行所需要的功能,该事件处理方法将在一个单独的线程中运行."
            注释 = "  注意: 在本事件的接收处理方法中,必须尽可能快地频繁调用本对象的\"是否需要退出\"方法,"
            注释 = "一旦发现该方法返回真,则需要尽快结束执行并返回,否则将导致另一线程中的\"停止\"方法调用" 注释 = "进入死等状态." 折叠>
    参数 来源对象 <类型 = 线程类 注释 = "提供事件产生的具体来源对象">
    参数 标记值 <类型 = 整数 注释 = "用户调用\"挂接事件\"命令时所提供的\"标记值\"参数值,非此方式挂接事件则本参数值固定为0.">
    {
        如果 (来源对象 == 成_处理任务)
        {
            判断循环 (来源对象.是否需要退出 (0) == 假)
            {
                成_任务锁.加锁 ()
                如果 (成_任务队列.取成员数 () > 0)
                {
                    如果 (成_空闲对象.取成员数 () > 0)
                    {
                        变量 任务 <类型 = 网络访问参数类>
                        变量 传输句柄 <类型 = 变整数>
                        任务 = 成_任务队列.取首成员 ()
                        成_任务队列.弹出首部 ()
                        传输句柄 = 成_空闲对象.取成员 (0)
                        成_工作对象.加入成员 (传输句柄)
                        成_空闲对象.删除成员 (0)
                        成_任务锁.解锁 ()
                        分支判断 (任务.访问类型)
                        {
                            分支 (0)  // 网页访问
                            {
                                设置网页访问 (读指针处对象 (传输句柄, Curl传输类), 任务)

                            }
                            分支 (1)  // 下载
                            {
                                设置下载 (读指针处对象 (传输句柄, Curl传输类), 任务)

                            }
                        }
                        成_任务队列.释放空闲内存 ()
                    }
                    否则
                    {
                        成_任务锁.解锁 ()
                        延时 (20)
                    }
                }
                否则
                {
                    成_任务锁.解锁 ()
                    延时 (20)

                }
                // 判断循环尾
            }
        }
        否则 (来源对象 == 成_处理响应)
        {
            变量 当前传输数量 <类型 = 整数>
            变量 完成任务数 <类型 = 整数>
            判断循环 (来源对象.是否需要退出 (0) == 假)
            {
                成_多句柄锁.加锁 ()
                Curl传输类.发送多会话请求 (成_多会话句柄, 取变量地址 (当前传输数量))
                成_多句柄锁.解锁 ()
                如果 (Curl传输类.轮询 (成_多会话句柄, 成_轮询时间, 取变量地址 (完成任务数)) == 0)
                {
                    变量 剩余消息数 <类型 = 整数>
                    循环判断首 ()
                    {
                        变量 会话信息 <类型 = CURL信息类>
                        成_多句柄锁.加锁 ()
                        剩余消息数 = Curl传输类.取多会话信息 (成_多会话句柄, 会话信息)
                        成_多句柄锁.解锁 ()
                        如果 (会话信息.信息 == 1)
                        {
                            变量 会话句柄 <类型 = 变整数>
                            变量 对象指针 <类型 = 变整数>
                            会话句柄 = 会话信息.会话句柄
                            对象指针 = 成_会话句柄表.取值 (会话句柄)
                            如果 (对象指针 == 0)
                            {
                                跳出循环
                            }
                            // 获取传输响应
                            分支判断 (读指针处对象 (对象指针, Curl传输类).成_访问类型)
                            {
                                分支 (0)  // 网页访问
                                {
                                    变量 访问响应 <类型 = 网页访问响应类>
                                    取网页访问响应 (读指针处对象 (对象指针, Curl传输类), 访问响应)
                                    网页访问完毕 (读指针处对象 (对象指针, Curl传输类), 读指针处对象 (对象指针, Curl传输类).取上次请求地址 (), 访问响应)

                                }
                                分支 (1)  // 下载
                                {
                                    如果 (读指针处对象 (对象指针, Curl传输类).成_文件操作.是否已打开 () == 真)
                                    {
                                        变量 下载响应 <类型 = 网络下载响应类>
                                        取下载响应 (读指针处对象 (对象指针, Curl传输类), 下载响应)
                                        下载完毕 (读指针处对象 (对象指针, Curl传输类), 读指针处对象 (对象指针, Curl传输类).取上次请求地址 (), 下载响应)
                                    }
                                    否则  // 仅获取了响应头,需要继续下载主体
                                    {
                                        如果 (继续下载 (读指针处对象 (对象指针, Curl传输类)) == 真)
                                        {
                                            到循环尾
                                        }
                                        否则
                                        {
                                            变量 下载响应 <类型 = 网络下载响应类>
                                            取下载响应 (读指针处对象 (对象指针, Curl传输类), 下载响应)
                                            下载完毕 (读指针处对象 (对象指针, Curl传输类), 读指针处对象 (对象指针, Curl传输类).取上次请求地址 (), 下载响应)

                                        }

                                    }

                                }

                            }
                            // 移除会话句柄
                            成_多句柄锁.加锁 ()
                            读指针处对象 (对象指针, Curl传输类).移除会话句柄 (成_多会话句柄)
                            成_多句柄锁.解锁 ()
                            成_任务锁.加锁 ()
                            // 从工作对象中移除
                            成_工作对象.查找删除成员 (对象指针)
                            // 加入到空闲对象,等待复用
                            如果 (成_工作对象.取成员数 () + 成_空闲对象.取成员数 () >= 成_并发数.取值 ())
                            {
                                销毁对象指针 (对象指针)
                                成_会话句柄表.删除 (会话句柄)
                            }
                            否则
                            {
                                读指针处对象 (对象指针, Curl传输类).重置参数 ()
                                成_空闲对象.加入成员 (对象指针)
                            }
                            成_任务锁.解锁 ()

                        }

                    }
                    循环判断尾 (剩余消息数 > 0)

                }

            }

        }
        返回 (0)
    }

    方法 下载写入数据回调 <静态 类型 = 变整数 注释 = "\"置写入数据回调函数\"对应的方法模板." 注释 = "注意:定义该回调方法时务必定义为\"CDECL\"调用方式."
            返回值注释 = "返回所处理的数据长度,返回0x10000001可暂停传输." 折叠 @视窗.前缀文本 = "@CDECL">
