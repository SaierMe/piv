类 IM输入框回调数据指针 <公开 基础类 = PIV指针模板 注释 = "ImGuiInputTextCallbackData" 折叠
        @别名 = "ImGuiInputTextCallbackData*" @模板实现类 = "IM输入框回调数据指针" @别名类型 = 本地参考类型 @类用途 = 禁止创建对象>
{
    变量 上下文 <公开 类型 = IM上下文指针 注释 = "父窗口的上下文" @输出名 = "Ctx">
    变量 事件标志 <公开 类型 = 整数 注释 = "(只读)为\"IM输入框标志\"中以\"回调\"结束的常量之一" @输出名 = "EventFlag">
    变量 标志 <公开 类型 = 整数 注释 = "(只读)调用\"xx输入框\"时传递的标志,请参考\"IM输入框标志\"中的常量." @输出名 = "Flags">
    变量 用户数据 <公开 类型 = 通用指针 注释 = "(只读)调用\"xx输入框\"时传递的用户数据" @输出名 = "UserData">
    变量 事件字符 <公开 类型 = IM字符 注释 = "(读写)输入的字符" 注释 = "[字符过滤回调] 可以替换字符" @输出名 = "EventChar">
    变量 事件按键 <公开 类型 = 整数 注释 = "(只读)请参考\"IM按键码\"的常量" 注释 = "[完成回调、历史记录回调]" @输出名 = "EventKey">
    变量 缓冲区 <公开 类型 = 通用指针 注释 = "(读写)文本缓冲区(char*)" 注释 = "[重设大小回调] 可以替换文本指针(该指针等于你输入的文本)"
            注释 = "[完成回调、历史记录回调、始终回调] 只能修改指针数据,不允许替换指针本身 (该指针指向内部数据)" @输出名 = "Buf">
    变量 文本长度 <公开 类型 = 整数 注释 = "(读写)文本长度(以字节为单位)" 注释 = "[重设大小回调、完成回调、历史记录回调、始终回调] 不包含结尾零字符的文本长度"
            @输出名 = "BufTextLen">
    变量 缓冲区长度 <公开 类型 = 整数 注释 = "(读写)文本缓冲区的长度(以字节为单位)" 注释 = "[重设大小回调、完成回调、历史记录回调、始终回调] 包含结尾零字符的文本缓冲区长度"
            @输出名 = "BufSize">
    变量 更新缓冲区 <公开 类型 = 逻辑型 注释 = "(只写)如果你修改了缓冲区数据和缓冲区长度,必须将本成员设置为\"真\"" 注释 = "[完成回调、历史记录回调、始终回调]"
            @输出名 = "BufDirty">
    变量 光标位置 <公开 类型 = 整数 注释 = "(读写)[完成回调、历史记录回调、始终回调]" @输出名 = "CursorPos">
    变量 选区起点 <公开 类型 = 整数 注释 = "(读写)等于\"选区终点\"表示无选区" 注释 = "[完成回调、历史记录回调、始终回调]" @输出名 = "SelectionStart">
    变量 选区终点 <公开 类型 = 整数 注释 = "(读写)[完成回调、历史记录回调、始终回调]" @输出名 = "SelectionEnd">

    方法 删除字符 <公开 静态 注释 = "删除文本缓冲区里的部分字符." 注释 = "如果需要修改文本缓冲区,请优先使用本方法." 折叠 @嵌入式方法 = "">
    参数 本结构指针 <类型 = IM输入框回调数据指针>
    参数 起始位置 <类型 = 整数 注释 = "所欲删除的起始索引位置">
    参数 删除长度 <类型 = 整数 注释 = "所欲删除的字节数">
    {
        @ @<本结构指针>->DeleteChars(@<起始位置>, @<删除长度>)
    }

    方法 插入字符 <公开 静态 注释 = "在文本缓冲区中插入字符串." 注释 = "如果需要修改文本缓冲区,请优先使用本方法." 折叠>
    参数 本结构指针 <类型 = IM输入框回调数据指针>
    参数 插入位置 <类型 = 整数 注释 = "所欲插入的索引位置">
    参数 文本指针 <类型 = 变整数 注释 = "所欲插入的UTF-8文本指针">
    参数 插入长度 <类型 = 整数 注释 = "所欲插入的字节数">
    {
        @ @<本结构指针>->InsertChars(@<插入位置>, (const char*)@<文本指针>, (const char*)@<文本指针> + @<插入长度>);
    }

    方法 全选 <公开 静态 注释 = "全选输入框的文本" 折叠 @嵌入式方法 = "">
    参数 本结构指针 <类型 = IM输入框回调数据指针>
    {
        @ @<本结构指针>->SelectAll()
    }

    方法 取消选择 <公开 静态 注释 = "取消选择输入框中的文本" 折叠 @嵌入式方法 = "">
    参数 本结构指针 <类型 = IM输入框回调数据指针>
    {
        @ @<本结构指针>->ClearSelection()
    }

    方法 是否有选区 <公开 静态 类型 = 逻辑型 注释 = "返回是否有被选定的文本" 折叠 @嵌入式方法 = "">
    参数 本结构指针 <类型 = IM输入框回调数据指针>
    {
        @ @<本结构指针>->HasSelection()
    }
}

