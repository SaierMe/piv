    {
        @ WINHTTP_AUTOPROXY_OPTIONS AutoProxyOptions;
        @ DWORD dwRes = WinHttpGetProxyForUrlEx ((HINTERNET)@<解析器句柄>, (PCWSTR)@<URL>.GetText (),
